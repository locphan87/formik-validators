{
  "name": "formik-validators",
  "version": "0.0.8",
  "description": "Reusable validators for formik",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:phanhoangloc/formik-validators.git"
  },
  "scripts": {
    "typecheck": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "prettier": "prettier --write '{src,tests}/**/*.ts'",
    "prebuild": "rm -rf dist && yarn prettier",
    "build": "tsc",
    "test": "jest ./tests/",
    "test:watch": "jest ./tests/ --watch"
  },
  "keywords": [
    "react",
    "form",
    "formik",
    "validators"
  ],
  "author": "Loc Phan <phanhoangloc@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "23.3.12",
    "@types/ramda": "0.25.38",
    "@types/react": "16.7.18",
    "formik": "1.4.2",
    "jest": "23.6.0",
    "prettier": "1.15.3",
    "ramda": "0.26.1",
    "ramda-adjunct": "2.10.0",
    "react": "16.7.0",
    "ts-jest": "23.10.5",
    "typescript": "3.2.2"
  }
}
